    {
      "parameters": {
        "functionCode": "// Define the list of free providers\nconst freeDomains = ['gmail.com', 'yahoo.com', 'hotmail.com', 'outlook.com', 'icloud.com'];\n\n// Get all items from the previous node\nconst items = $input.all();\n\n// Map through each item to transform the data\nreturn items.map(item => {\n    // 1. Safely grab the email from the previous node's JSON\n    const email = item.json.email || \"\";\n    \n    // 2. Extract the domain (with a fallback if email is missing or invalid)\n    const domain = email.includes('@') ? email.split('@')[1].toLowerCase() : 'unknown';\n\n    // 3. Add the new properties to the JSON object\n    // Business if: not in the free list AND not 'unknown'\n    item.json.isBusiness = !freeDomains.includes(domain) && domain !== 'unknown';\n    item.json.domain = domain;\n\n    return item;\n});"
      },
